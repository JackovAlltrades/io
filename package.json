{
  "name": "io-gui",
  "version": "1.0.2",
  "description": "Lightweight UI framework for reactive web applications.",
  "jsnext:main": "dist/io.js",
  "module": "dist/io.js",
  "files": [
    "src",
    "dist/*.js",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "jspm": {
    "files": [
      "src",
      "dist/*.js",
      "package.json",
      "LICENSE",
      "README.md"
    ],
    "directories": {}
  },
  "scripts": {
    "lint": "eslint src",
    "rollup": "rollup -c",
    "uglify": "uglifyjs dist/io.js -cmo dist/io.js --comments",
    "uglify-elements": "uglifyjs dist/io-elements.js -cmo dist/io-elements.js --comments",
    "uglify-extras": "uglifyjs dist/io-extras.js -cmo dist/io-extras.js --comments",
    "uglify-all": "npm run uglify && npm run uglify-elements && npm run uglify-extras",
    "build": "npm run lint && npm run rollup && npm run uglify-all",
    "travis": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/io-gui/io.git"
  },
  "keywords": [
    "io",
    "gui",
    "ui",
    "ux",
    "navigation",
    "menu",
    "io-gui",
    "web components",
    "custom elements",
    "reactive",
    "data binding"
  ],
  "author": "Aleksandar (Aki) RodiÄ‡",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/io-gui/io/issues"
  },
  "homepage": "https://io-gui.dev",
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^6.0.1",
    "rollup": "^1.16.7",
    "uglify-es": "^3.3.10"
  }
}
