{
  "name": "io-gui",
  "version": "2.5.0",
  "description": "UI library for web applications.",
  "jsnext:main": "dist/io.js",
  "module": "dist/io.js",
  "files": [
    "src",
    "dist/*.js",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "jspm": {
    "files": [
      "src",
      "dist/*.js",
      "package.json",
      "LICENSE",
      "README.md"
    ],
    "directories": {}
  },
  "scripts": {
    "uglify": "uglifyjs dist/io.js -cmo dist/io.js --comments && uglifyjs dist/io-extras.js -cmo dist/io-extras.js --comments",
    "build": "eslint src && rollup -c && npm run uglify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/io-gui/io.git"
  },
  "keywords": [
    "io",
    "gui",
    "ui",
    "ux",
    "navigation",
    "menu",
    "io-gui"
  ],
  "author": "Aleksandar (Aki) RodiÄ‡",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/io-gui/io/issues"
  },
  "homepage": "https://io-gui.dev",
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "eslint": "^4.19.1",
    "rollup": "^0.59.4",
    "uglify-es": "^3.3.10"
  }
}
